def alpha(name: str = "Jules") -> str
    echo "Hello, #{name}!"
end

def bravo(name = "Jules")
    echo "Hello, #{name}!"
end

def run_command(args: array(str)) -> str
    if args.length == 0
        return "No command provided."
    else
        return "Command executed: #{args.join(' ')}"
    end
end

def run(argv: nil, stdout: nil, stderr: nil) -> int
    argv = argv || ARGV
    stdout = stdout || STDOUT
    stderr = stderr || STDERR

    return 1
end

