#[
    Range Example
]#

# Inclusive range includes both endpoints.
inclusive = 1..3

echo inclusive.first
echo inclusive.last
echo inclusive.includes?(2)

# Exclusive range skips the end value.
exclusive = 1...4

echo exclusive.last
echo exclusive.includes?(4)

# Ranges support iteration with blocks.
('a'..'c').each do |letter|
    echo "Letter: #{letter}"
end

range = 1..5
echo range.to_a

r = 1...5
echo r.to_a
