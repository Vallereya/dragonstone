message = "Hello!"
e! message

name = "Jules"
e! name

# Using typeof
e! typeof(message)

x = 42
e! typeof(x)

y = 3.14
e! typeof(y)

flag = true
e! typeof(flag)

# Complex expressions
a = 10
b = 5
e! a + b

e! a + b * 2

# typeof with expressions
e! typeof(a + b)
e! typeof("test")

# Combining typeof with variables
result = a * b
e! result
e! typeof(result)
