#[
    Just a few.

    ** 	    exponentiation
    &** 	wrapping exponentiation

    * 	    multiplication
    &* 	    wrapping multiplication

    / 	    division
    // 	    floor division
    
    % 	    modulus
]#

a = 5
b = 10
c = 15
d = 20
e = 25

# Addition
add = a + b
echo add

# Subtraction
sub = e - a
echo sub

# Multiplication
multi = b * c
echo multi

# Division
div = d / a
echo div

# Floor Division
floor_div = e // b
echo floor_div
