# Example One
my_tuple = {1, "Hey", 'x'}
puts my_tuple[0]    # => 1
puts my_tuple[1]    # => Hey
puts my_tuple[2]    # => x

# Example Two
person = {name: "V", age: 30}
puts person[:name]  # => V
puts person[:age]   # => 30

# Example Three
person = {name: str = "V", age: int = 30}
puts person[:name]  # => V
puts person[:age]   # => 30
